{
	"$schema": "https://biomejs.dev/schemas/2.2.6/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": true,
		"includes": [
			"**",
			"!**/node_modules",
			"!**/package-lock.json",
			"!**/yarn.lock"
		]
	},
	"assist": { "actions": { "source": { "organizeImports": "on" } } }
}
